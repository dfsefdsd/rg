	<entry key="sequenceName">seq_im$table</entry>
	<entry key="autoGenFile">true</entry>
	<entry key="tableName">im$table</entry>
	<entry key="coverFile">true</entry>
	<entry key="queryFields">table_id</entry>
	<entry key="updateFields">table_name</entry>
                                    

	<!-- 需要移除的表名前缀,使用逗号进行分隔多个前缀,示例值: t_,v_ -->
