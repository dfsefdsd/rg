<?xml version="1.0" encoding="GBK"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
	<comment>
		代码生成器配置文件: 
		1.会为所有的property生成property_dir属性,如pkg=com.company => pkg_dir=com/company
		2.可以引用环境变量: ${env.JAVA_HOME} or System.properties: ${user.home},property之间也可以相互引用
	
	<!-- oracle需要指定jdbc.schema,其它数据库忽略此项配置  -->
	jdbc.schema
	mvoPackage
	sequenceName
	autoGenFile
	coverFile
	tableName
	mvoPackage
	soaCorePackage
	soaSrcPackage
	queryFields
	updateFields
	sql
	countSql
	byIdFields
	</comment>

	<!-- jsp namespace: web/${namespace}/${className}/list.jsp -->
	<entry key="namespace">pages</entry>
	<entry key="sql">select * from im$table i where 1=1 </entry>
	<entry key="byIdFields">table_id</entry>
	<entry key="countSql">select count(1) from im$table i where 1=1 </entry>
